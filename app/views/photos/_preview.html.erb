<p id="notice"><%= notice %></p>

<h1>Listing Photos</h1>
<%= link_to 'Add', new_place_photo_path, class: 'btn btn-default' %></td>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% if @photos.nil?%>
       <h3>No Images</h3>
       
      
    <%else%>
        
          <%@photos.each do |photo| %>
          
            <tr>
              <td><%=image_tag(photo.image.url(:thumb)) %> </td>
              <td><%= photo.name %></td>
              
            
            </tr>
          <% end %>
    <%end%>
  </tbody>
</table>

<br>


